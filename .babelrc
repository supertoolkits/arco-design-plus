{
  "env": {
    "development": {
      "plugins": [ "istanbul" ]
    }
  },
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "macros",
    "@emotion",
    [
      "babel-plugin-import",
      {
        "libraryName": "@arco-design/web-react",
        "libraryDirectory": "es",
        "camel2DashComponentName": false,
        "style": true
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}
